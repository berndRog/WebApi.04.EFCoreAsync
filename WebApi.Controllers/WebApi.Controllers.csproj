		<RootNamespace>WebApi</RootNamespace>
		<Nullable>enable</Nullable>
		<ImplicitUsings>disable</ImplicitUsings>
	</PropertyGroup>

	<ItemGroup>
	  <ProjectReference Include="..\WebApi.Core\WebApi.Core.csproj" />
